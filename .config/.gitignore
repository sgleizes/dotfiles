# Exceptions for directories with uninteresting contents.
/Nextcloud/**
!/Nextcloud/nextcloud.cfg
/beets/**
!/beets/config.yaml
/elinks/**
!/elinks/elinks.conf
/nnn/**
!/nnn/plugins/
!/nnn/plugins/**
/syncthing/**
